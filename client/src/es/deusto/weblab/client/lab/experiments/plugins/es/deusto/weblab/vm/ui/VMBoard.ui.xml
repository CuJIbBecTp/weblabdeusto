<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:w="urn:import:es.deusto.weblab.client.ui.widgets">
    
    
    <g:VerticalPanel ui:field='widget' width='100%' horizontalAlignment='ALIGN_CENTER'>

				<g:HorizontalPanel borderWidth="0" verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER" width="100%">
					<w:WlTimer ui:field="timer" height="50%" width="100%" styleName="wl-time_remaining"></w:WlTimer>
				</g:HorizontalPanel>

				<w:WlWaitingLabel ui:field="messages" horizontalAlignment="ALIGN_CENTER"/>
				
				<g:Label ui:field="url" horizontalAlignment="ALIGN_CENTER"></g:Label>

	</g:VerticalPanel>
	

</ui:UiBinder>