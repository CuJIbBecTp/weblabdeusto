<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:my='urn:import:es.deusto.weblab.client.ui.themes.es.deusto.weblab.defaultweb.widgets'
    xmlns:w="urn:import:es.deusto.weblab.client.ui.widgets">
    
    
    
	<w:WlVerticalPanel>
		
		
		<g:AbsolutePanel ui:field="navigationPanel">
		</g:AbsolutePanel>
		
		<g:Label ui:field="contentTitleLabel" styleName="title-label" horizontalAlignment='ALIGN_CENTER'>
		</g:Label>
		
		<g:Label ui:field="generalErrorLabel" styleName="wl-error_message">
		</g:Label>
		
		<w:WlVerticalPanel ui:field="listExperimentsPanel" width="100%" horizontalAlignment="ALIGN_CENTER">
		
			<!-- Content is dynamically generated in code -->
			<g:FlexTable ui:field="experimentsTable" cellPadding='10' borderWidth='0'>
			</g:FlexTable>
		
		</w:WlVerticalPanel>
		
		
		<w:WlWaitingLabel ui:field='waitingLabel'>
		</w:WlWaitingLabel>
		
	
	</w:WlVerticalPanel>

</ui:UiBinder>