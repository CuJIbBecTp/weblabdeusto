    xmlns:w="urn:import:es.deusto.weblab.client.ui.widgets">
    
	<w:WlVerticalPanel>
		
		<g:Label ui:field="contentTitleLabel" styleName="title-label" horizontalAlignment='ALIGN_CENTER'></g:Label>
		
