<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:my='urn:import:es.deusto.weblab.client.ui.themes.es.deusto.weblab.defaultmobile.widgets'
    xmlns:w="urn:import:es.deusto.weblab.client.ui.widgets">
 
    <ui:with field="i18n" type="es.deusto.weblab.client.ui.themes.es.deusto.weblab.defaultmobile.i18n.IWebLabDeustoThemeMessages"/>
    
	<w:WlVerticalPanel spacing="10">
		
		<g:Label text="{i18n.myExperiments}" styleName="title-label" horizontalAlignment='ALIGN_CENTER' />
		
		<g:Label ui:field="generalErrorLabel" styleName="wl-error_message" />
		
		<g:Grid ui:field="experimentsTable" width="100%" cellPadding="5" cellSpacing="0" />
		
		<w:WlWaitingLabel ui:field='waitingLabel' />
		
	</w:WlVerticalPanel>

</ui:UiBinder>